$milis_hello $mol_view
	sub /
		<= Name $mol_string
			hint @ \Name
			value?val <=> name?val \
		<= message \
		<= ShowTutorial $mol_button_major
			title @ \Show
			event_click?val <=> toggleTutorial?val null
		<= tutorial 
		<= Helper $milis_helper
			items <= tourItems /$milis_helper_TourItem
			endTour <= toggleTutorial
