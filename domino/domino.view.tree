$milis_domino $mol_page
	title \Домино
	body /
		<= Dice1 $milis_domino_dice
			first 0
			second 1
		<= Dice2 $milis_domino_dice
			first 2
			second 3
		<= Dice3 $milis_domino_dice
			first 4
			second 5
		<= Dice4 $milis_domino_dice
			first 6
			second 6

$milis_domino_dice $mol_view
	sub /
		<= First $mol_view
			attr *
				first <= first 0
		<= Second $mol_view
			attr *
				second <= second 0
